--===============================================--
-- By VoltenX
-- // Services

local Players = game:GetService("Players")
local Workspace = game:GetService("Workspace")
local RS = game:GetService("ReplicatedStorage")

--===============================================--

-- // Vars

local player = Players.LocalPlayer
local character = player.Character
local hpart = character.HumanoidRootPart

--===============================================--

local OldNameCall
    OldNameCall = hookmetamethod(game, "__namecall", newcclosure(function(self, ...)
    local method = getnamecallmethod()
    local args = {...}
    if not checkcaller() and self.name == "FallDamage" and method == "FireServer" then
        return nil
    end
    return OldNameCall(self, ...)
end))

-- =============================================================--
local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("VoltenX", "Sentinel")
-- =============================================================--

local Tab = Window:NewTab("Config")
local Config = Tab:NewSection("Config")
local Time = Tab:NewSection("Time")
local Misc = Tab:NewSection("Misc")

Config:NewKeybind("KeybindButton", "KeybindButton", Enum.KeyCode.RightControl, function()
	Library:ToggleUI()
end)

Config:NewButton("Infinite Yield", "Infinite Yield", function()
	loadstring(game:HttpGet("https://raw.githubusercontent.com/EdgeIY/infiniteyield/master/source"))()
end)

Config:NewButton("Remote Spy", "Remote Spy", function()
	loadstring(game:HttpGet("https://github.com/exxtremestuffs/SimpleSpySource/raw/master/SimpleSpy.lua"))()
end)

Time:NewButton("Foundation", "Foundation Button", function()
	local args = {[1] = "Foundation",[2] = true}
	game:GetService("ReplicatedStorage").Remotes.ChangeTeam:InvokeServer(unpack(args))
	game:GetService("ReplicatedStorage").Remotes.RespawnPlayer:FireServer()
end)

Time:NewButton("Prisoner", "Foundation Button", function()
	local args = {[1] = "Prisoner",[2] = false}
	game:GetService("ReplicatedStorage").Remotes.ChangeTeam:InvokeServer(unpack(args))
	game:GetService("ReplicatedStorage").Remotes.RespawnPlayer:FireServer()
end)

Misc:NewButton("Disable Jump Stamina", "Disable Jump Stamina drain", function()
	local OldNameCall
    OldNameCall = hookmetamethod(game, "__namecall", newcclosure(function(self, ...)
    local method = getnamecallmethod()
    local args = {...}
    if not checkcaller() and self.name == "_" and method == "Fire" and args[1] == "SubtractStamina" then
        return nil
    end
    return OldNameCall(self, ...)
end))
end)

Misc:NewButton("Destroy Tesla Gate", "Tesla Gate", function()
	local function destroyteslagates()
		for i,v in pairs(Workspace:GetChildren()) do
			if v.Name == "TeslaGate" then
				v:Destroy()
			end
		end
	end
end)

Misc:NewButton("Remove Textures", "Remove Texture", function()
	local ws = game:GetService("Workspace")

	for i,v in pairs(ws:GetDescendants()) do
	   if v:IsA("Texture") then
		   v:Destroy()
	   end
	end
end)

-- =============================================================--

local Tab = Window:NewTab("Teleporte")
local tp1 = Tab:NewSection("Misc")
local tp = Tab:NewSection("Teleportes")
local tp2 = Tab:NewSection("Light") 


tp1:NewButton("Escape Chaos Insurgency", "Escape Button", function()
	tweenService, tweenInfo = game:GetService("TweenService"), TweenInfo.new(1, Enum.EasingStyle.Linear)

	tween = tweenService:Create((hpart), tweenInfo, {CFrame = CFrame.new(344, 8, 161)})
	tween:Play()
	wait(3)
	RS.Remotes.Interact:FireServer(Workspace.Elevators.GateAToSurface1.Call)
	wait(5)

	tween2 = tweenService:Create((hpart), tweenInfo, {CFrame = CFrame.new(344, 8, 143)})
	tween2:Play()
	wait(3)

	RS.Remotes.Interact:FireServer(Workspace.Elevators.GateAToSurface1.UseElevator)
	wait(5)

	tween3 = tweenService:Create((hpart), tweenInfo, {CFrame = CFrame.new(208, 209, 1464)})
	tween3:Play()
	wait(2)

	tween4 = tweenService:Create((hpart), tweenInfo, {CFrame = CFrame.new(490.485901, 209.402817, 1418.58826)})
	tween4:Play()
	wait(2)

	for i = 1, 8 do
		game.Players.LocalPlayer.Character.Humanoid:ChangeState(Enum.HumanoidStateType.Jumping)
		wait(1)
	end
end)

tp1:NewButton("Omega Warhead Activation", "Omega Warhead Activation", function()
	local CFrameEnd = CFrame.new(564, -175, 13) -- Place your coords in here
	local Time = 3.5 -- Time in seconds
	local tween =  game:GetService("TweenService"):Create(game.Players.LocalPlayer.Character.HumanoidRootPart, TweenInfo.new(Time), {CFrame = CFrameEnd})
	tween:Play()
	tween.Completed:Wait()
end)

